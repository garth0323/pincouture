<section class="block">	
	<div id="carousel-example" class="carousel slide" data-ride="carousel">
	    <ol class="carousel-indicators">
	        <li data-target="#carousel-example" data-slide-to="0" class="active"></li>
	        <li data-target="#carousel-example" data-slide-to="1"></li>
	        <li data-target="#carousel-example" data-slide-to="2"></li>
	        <li data-target="#carousel-example" data-slide-to="3"></li>
	    </ol>

	    <div class="carousel-inner">
	        <div class="item active">
	            <%= image_tag("embrace-jumbotron.png", alt: "Embrace Couture") %>
	            <% if !logged_in? %>
	            <div class="carousel-caption text-center">
	        			<p>
		      				<%= link_to "Log in", login_path, class: "btn btn-default btn-lg"  %>
		      				<%= link_to "Sign up", new_user_path, class: "btn btn-info btn-lg" %>
		    				</p>
	      			</div>
	      			<% end %>
	        </div>
	        <div class="item">
	            <img src="http://destinationluxury.com/wp-content/uploads/2014/03/Screen-Shot-2014-03-07-at-12.27.48-PM-1024x400.jpg" alt="Band performing 2">
	            <% if !logged_in? %>
	            <div class="carousel-caption text-center">
	        			<p>
		      				<%= link_to "Log in", login_path, class: "btn btn-default btn-lg"  %>
		      				<%= link_to "Sign up", new_user_path, class: "btn btn-info btn-lg" %>
		    				</p>
	      			</div>
	      			<% end %>
	        </div>
	        <div class="item">
	            <img src="http://ellesaintdeveil.files.wordpress.com/2014/02/1994225-1024x682.jpg?w=1024&h=400&crop=1" alt="Band performing 3">
	            <% if !logged_in? %>
	            <div class="carousel-caption text-center">
	        			<p>
		      				<%= link_to "Log in", login_path, class: "btn btn-default btn-lg"  %>
		      				<%= link_to "Sign up", new_user_path, class: "btn btn-info btn-lg" %>
		    				</p>
	      			</div>
	      			<% end %>
	        </div>
	        <div class="item">
	            <img src="http://cdn.business2community.com/wp-content/uploads/2014/03/Shark-Tank.jpg" alt="Band performing 3">
	            <% if !logged_in? %>
	            <div class="carousel-caption text-center">
	        			<p>
		      				<%= link_to "Log in", login_path, class: "btn btn-default btn-lg"  %>
		      				<%= link_to "Sign up", new_user_path, class: "btn btn-info btn-lg" %>
		    				</p>
	      			</div>
	      			<% end %>
	        </div>
	    </div>

	    <a class="left carousel-control" href="#carousel-example" data-slide="prev">
	        <span class="glyphicon glyphicon-chevron-left"></span>
	    </a>
	    <a class="right carousel-control" href="#carousel-example" data-slide="next">
	        <span class="glyphicon glyphicon-chevron-right"></span>
	    </a>
	</div>
</section>